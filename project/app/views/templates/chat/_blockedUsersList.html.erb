			<div class="flex flex-row items-center p-2">
				<img
					class="w-12 h-12 rounded-full p-2"
					src="{{ avatar_url }}"
				/>
				<div class="w-48">
					{{login}}
				</div>
				 <svg data-user_login="{{login}}" data-user_id="{{id}}" id="unblock-user" class="ml-2 w-6 h-6 cursor-pointer" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M6 18L18 6M6 6l12 12"></path></svg>
