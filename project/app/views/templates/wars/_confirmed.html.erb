<script type="text/html" id="confirmedWarTemplate">
	<h1 style="margin-top:10px;" class="justify-center flex flex-row  text-white"> Your guild is currently at war with: </h1>
	<div>
		<img style="margin-left: auto; margin-right: auto; margin-top:10px" width="100" height="100" class="flex flex-row rounded-full p-2" src="{{img}}" />
	</div>
	<a style="margin-top:10px; text-decoration: underline;" href="{{url}}" class="justify-center flex flex-row text-white">{{name}} </a>
	<h1 style="margin-top:10px; font-size: 15px;" class="flex flex-row text-white ml-5"> Prize : {{war.prize}} points </h1>
	<div id="confirmed" style="display:none">
		<h1 style="margin-top:10px; margin-down:10px; font-size: 15px;" class="justify-center flex flex-row text-white"> Starts {{war.countStart}} </h1>
		<h1 style="margin-top:10px; margin-down:10px; font-size: 15px;" class="flex flex-row text-white ml-5 mb-5"> End: {{war.end}} </h1>
	</div>
	<div id="started" style="display:none">
		<h1 style="margin-top:10px; margin-down:10px; font-size: 15px;" class="justify-center flex flex-row text-white"> Ends {{war.countEnd}} </h1>
		<h1 style="margin-top:10px; margin-down:10px; font-size: 15px;" class="flex flex-row text-white ml-5 mb-5"> Started on: {{war.start}} </h1>
		<button id="wartime-btn" style="margin-left:155px;" class="btn-primary mt-5 mb-10" type="button">Start war time!</button>
	</div>
</script>
