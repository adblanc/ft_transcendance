<script type="text/html" id="confirmedWarTemplate">
	<h1 style="margin-top:10px;" class="justify-center flex flex-row  text-white"> Your guild is currently at war with: </h1>
	<div>
		<img style="margin-left: auto; margin-right: auto; margin-top:10px" width="100" height="100" class="flex flex-row rounded-full p-2" src="{{war.guilds.{{id}}.img_url}}" />
	</div>
	<a style="margin-top:10px; text-decoration: underline;" href="/guild/{{war.guilds.{{id}}.id}}" class="justify-center flex flex-row text-white">{{war.guilds.{{id}}.name}} </a>
	<h1 style="margin-top:10px; font-size: 15px;" class="flex flex-row text-white ml-5"> Prize : {{war.prize}} points </h1>
	<h1 style="margin-top:10px; margin-down:10px; font-size: 15px;" class="flex flex-row text-white ml-5"> Start: {{war.start}} </h1>
	<h1 style="margin-top:10px; margin-down:10px; font-size: 15px;" class="flex flex-row text-white ml-5 mb-5"> End: {{war.end}} </h1>
	<h1 style="margin-top:10px; margin-down:10px; font-size: 15px;" class="justify-center flex flex-row text-white"> status: {{war.status}} </h1>
	<button id="accept-btn" style="margin-left:155px" class="btn-primary mt-5" type="button">Start war time!</button>
</script>
