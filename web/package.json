{
    "name": "parcel-tailwindcss-ts",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "dev": "yarn clean && parcel src/index.html --port 3000",
        "build": "tsc --noEmit && yarn lint && yarn clean && yarn build:parcel",
        "build:parcel": "parcel build src/index.html --public-url ./",
        "lint": "eslint --ext ts src",
        "clean": "rimraf ./dist ./.cache"
    },
    "dependencies": {
        "@tailwindcss/custom-forms": "^0.2.1",
        "axios": "^0.21.0",
        "backbone": "^1.4.0",
        "jquery": "^3.5.1",
        "mustache": "^4.0.1",
        "tailwindcss": "^1.9.6",
        "toastify-js": "^1.9.3",
        "underscore": "^1.11.0"
    },
    "devDependencies": {
        "@types/backbone": "^1.4.5",
        "@types/jquery": "^3.5.4",
        "@types/mustache": "^4.0.1",
        "@types/node": "^14.14.6",
        "@types/parcel-env": "^0.0.0",
        "@types/underscore": "^1.10.24",
        "autoprefixer": "^10.0.1",
        "eslint": "^7.12.1",
        "parcel-bundler": "^1.12.4",
        "postcss": "^8.1.4",
        "prettier": "^2.1.2",
        "rimraf": "^3.0.2",
        "typescript": "^4.0.5"
    }
}
